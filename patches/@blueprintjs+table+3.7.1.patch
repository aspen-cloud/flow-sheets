+import { isFunction } from "../utils";
+// tslint:disable-next-line:ban-types
+export function safeInvokeMember(obj, key, ...args) {
+    if (obj != null) {
+        const member = obj[key];
+        if (isFunction(member)) {
+            return member(...args);
+        }
+    }
+    return undefined;
